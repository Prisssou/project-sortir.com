{{ form_start(form) }}
<div class="small-margin contenantForm">
    <div class="row">
        <div class="col-md-8 .ml-md-auto">
            <div class="no-padding input-group">
                {#{{ form_label(form.motCle) }}<br>#}
                {{ form_widget(form.motCle) }}
                {{ form_errors(form.motCle) }}
                <div class="input-group-append">
                    <div class="input-group-text"><i class="fa fa-search"></i></div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-8 .ml-md-auto">
            {{ form_widget(form.motCle) }}<br>
            <div class="form-group">
                <div class="input-group date" id="datetimepicker2a" data-target-input="nearest">
                    {{ form_label(form.beginDate) }}
                    {{ form_widget(form.beginDate) }}
                    {{ form_errors(form.beginDate) }}
                    <div class="input-group-append" data-target="#datetimepicker2a" data-toggle="datetimepicker">
                        <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-lg-6">
            <div class="input-group date" id="datetimepicker2b" data-target-input="nearest">
                {{ form_label(form.endDate) }}
                {{ form_widget(form.endDate) }}
                {{ form_errors(form.endDate) }}
                <div class="input-group-append" data-target="#datetimepicker2b"
                     data-toggle="datetimepicker">
                    <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                </div>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="col-lg-6">
            {{ form_label(form.dureeMin) }}
            {{ form_widget(form.dureeMin) }}
            {{ form_errors(form.dureeMin) }}
        </div>
        <div class="col-lg-6">
            {{ form_label(form.dureeMax) }}
            {{ form_widget(form.dureeMax) }}
            {{ form_errors(form.dureeMax) }}<br>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-6">
            {{ form_label(form.orga) }}
            {{ form_widget(form.orga) }}
            {{ form_errors(form.orga) }}<br>


            {{ form_label(form.inscrit) }}
            {{ form_widget(form.inscrit) }}
            {{ form_errors(form.inscrit) }}<br>
        </div>
        <div class="col-lg-6">
            {{ form_label(form.notInscrit) }}
            {{ form_widget(form.notInscrit) }}
            {{ form_errors(form.notInscrit) }}<br>

            {{ form_label(form.passee) }}
            {{ form_widget(form.passee) }}
            {{ form_errors(form.passee) }}<br>

        </div>
    </div>
    <button type="submit" class="btn w-100 " style="background-color: #142340; color: #FFC952;">Rechercher</button>


</div>
{{ form_end(form) }}





